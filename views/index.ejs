                </div>
                <div class="form-group">
                    <label for="message">Message</label><br />
                    <textarea class="form-control" rows="3" id="message" placeholder="Send me a message..."></textarea>
                </div>
                <div>
                    <button type="submit" class="btn btn-default">Submit</button>
                </div>
            </form>
            <div class="col-sm-4">
                <!--blank-->
            </div>
